CFLAGS = -qixt=/ddLFLAGS = $(CFLAGS) -g -l=/h0/LIB/os9lib.lCC = ccODIR = .RDIR = RELSTDIR = ../CMDSPROGS = $(TDIR)/tosdir $(TDIR)/tosread $(TDIR)/toswrite $(TDIR)/tosdel \        $(TDIR)/tostype $(TDIR)/tosmd $(TDIR)/tosrd $(TDIR)/tosrenMDIR = tosdir.r getfat.r init.r search.r match.r convdate.r unixname.r \       subdir.r isdir.rMREAD = tosread.r getfat.r init.r search.r match.r unixname.r subdir.rMWRITE = toswrite.r getfat.r init.r search.r fixname.r unixname.r putfat.r \    subdir.r mkentry.r isdir.rMDEL = tosdel.r getfat.r init.r search.r match.r unixname.r putfat.r subdir.rMTYPE = tostype.r getfat.r init.r search.r match.r unixname.r subdir.rMMD = tosmd.r getfat.r init.r search.r fixname.r unixname.r putfat.r subdir.r \    mkentry.rMRD = tosrd.r getfat.r init.r search.r unixname.r putfat.r subdir.rMREN = tosren.r getfat.r init.r search.r fixname.r unixname.r putfat.r subdir.r \    isdir.rmake.date: $(PROGS) msdos.h           touch make.date          $(TDIR)/tosdir:   $(MDIR) msdos.h          chd $(RDIR); $(CC) $(MDIR) $(LFLAGS) -f=$@$(TDIR)/tosread:  $(MREAD) msdos.h          chd $(RDIR); $(CC) $(MREAD) $(LFLAGS) -f=$@$(TDIR)/toswrite: $(MWRITE) msdos.h          chd $(RDIR); $(CC) $(MWRITE) $(LFLAGS) -f=$@$(TDIR)/tosdel:   $(MDEL) msdos.h          chd $(RDIR); $(CC) $(MDEL) $(LFLAGS) -f=$@$(TDIR)/tostype:  $(MTYPE) msdos.h          chd $(RDIR); $(CC) $(MTYPE) $(LFLAGS) -f=$@$(TDIR)/tosmd:    $(MMD) msdos.h          chd $(RDIR); $(CC) $(MMD) $(LFLAGS) -f=$@$(TDIR)/tosrd:    $(MRD) msdos.h          chd $(RDIR); $(CC) $(MRD) $(LFLAGS) -f=$@$(TDIR)/tosren:   $(MREN) msdos.h          chd $(RDIR); $(CC) $(MREN) $(LFLAGS) -f=$@