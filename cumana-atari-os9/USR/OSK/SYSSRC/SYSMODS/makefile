## makefile for Cumana OS-9/68000 V2.2 implementation (drivers)## Created: 08/27/87## OSK implementation for the ATARI ST#RDIR   = RELSODIR   = ../CMDS/BOOTOBJSSYSLIB = -l=/h0/LIB/sys.lLFLAGS = -gRFLAGS = all.date: init cumana recco  @echo "ALL DONE"init: $(ODIR)/stinit.d0 $(ODIR)/stinit.h0 $(ODIR)/stsysgo $(ODIR)/stsysts  @echo "INIT DONE"cumana: $(ODIR)/stcon $(ODIR)/stmouse                                       \        $(ODIR)/stfd $(ODIR)/stfd.tos $(ODIR)/sthd                          \        $(ODIR)/stser $(ODIR)/stmidi $(ODIR)/stpar                          \        $(ODIR)/stclock $(ODIR)/stclock.rom                                 \        $(ODIR)/stclock.hc $(ODIR)/stclock.rom.hc  @echo "CUMANA DONE"recco: $(ODIR)/stvir $(ODIR)/st681  @echo "RECCO DONE"STVIR_OBJS = stvir.r stvir0.r stvirs.r$(ODIR)/stvir: $(STVIR_OBJS)  chd $(RDIR); l68 $(LFLAGS) -n=stvir -o=$@ $(SYSLIB) /h0/LIB/scfstat.l \                   $(STVIR_OBJS)STCON_OBJS = stcon.r stcon0.r stcon1.r stcon2.r stcons.r$(ODIR)/stcon: $(STCON_OBJS)  chd $(RDIR); l68 $(LFLAGS) -n=stcon -o=$@ $(SYSLIB) /h0/LIB/scfstat.l \                   $(STCON_OBJS)stcon.r: stcon.a  r68 stcon.a -o=$(RDIR)/stcon.rstcon0.r: stcon0.a  r68 stcon0.a -o=$(RDIR)/stcon0.rstcon1.r: stcon1.a  r68 stcon1.a -o=$(RDIR)/stcon1.rstcon2.r: stcon2.a  r68 stcon2.a -o=$(RDIR)/stcon2.rstcons.r: stcons.a  r68 stcons.a -o=$(RDIR)/stcons.r$(ODIR)/stm110: stm110.r stm1100.r  chd $(RDIR); l68 $(LFLAGS) -n=stm110 -o=$@ $(SYSLIB) /h0/LIB/scfstat.l \                   stm110.r stm1100.rstm110.r: stcon.a  r68 -A=M110L stcon.a -o=$(RDIR)/stm110.rstm1100.r: stcon0.a  r68 -A=M110L stcon0.a -o=$(RDIR)/stm1100.r$(ODIR)/st194: st194.r st1940.r  chd $(RDIR); l68 $(LFLAGS) -n=st194 -o=$@ $(SYSLIB) /h0/LIB/scfstat.l \                   st194.r st1940.rst194.r: stcon.a  r68 -A=M110L stcon.a -o=$(RDIR)/st194.rst1940.r: stcon0.a  r68 -A=M110L stcon0.a -o=$(RDIR)/st1940.r$(ODIR)/stfd: stfd.r  chd $(RDIR); l68 $(LFLAGS) -n=stfd -o=$@ $(SYSLIB) /h0/LIB/drvs2.l \                   stfd.rstfd.r: stfd.a  r68 stfd.a -o=$(RDIR)/$@ $(RFLAGS) -a=TOS=0 -a=STDDISK=0$(ODIR)/stfd.tos: stfd_tos.r  chd $(RDIR); l68 $(LFLAGS) -n=stfd -o=$@ $(SYSLIB) /h0/LIB/drvs2.l \                   stfd_tos.rstfd_tos.r: stfd.a  r68 stfd.a -o=$(RDIR)/$@ $(RFLAGS) -a=TOS=1 -a=STDDISK=0$(ODIR)/sthd: sthd.r  chd $(RDIR); l68 $(LFLAGS) -n=sthd -o=$@ $(SYSLIB) /h0/LIB/drvs20.l \                   sthd.r;sthd.r: sthd.a  r68 sthd.a -o=$(RDIR)/$@ $(RFLAGS)$(ODIR)/stser: stser.r  chd $(RDIR); l68 $(LFLAGS) -n=stser -o=$@ $(SYSLIB) /h0/LIB/scfstat.l \                   stser.rstser.r: stser.a  r68 stser.a -o=$(RDIR)/$@ -q$(ODIR)/st681: st681.r  chd $(RDIR); l68 $(LFLAGS) -n=st681 -o=$@ $(SYSLIB) /h0/LIB/scfstat.l \                   st681.r$(ODIR)/stmidi: stmidi.r  chd $(RDIR); l68 $(LFLAGS) -n=stmidi -o=$@ $(SYSLIB) /h0/LIB/scfstat.l \                   stmidi.r$(ODIR)/stpar: stpar.r  chd $(RDIR); l68 $(LFLAGS) -n=stpar -o=$@ $(SYSLIB) /h0/LIB/scfstat.l \                   stpar.r$(ODIR)/stmouse: stmouse.r  chd $(RDIR); l68 $(LFLAGS) -n=stmouse -o=$@ $(SYSLIB) /h0/LIB/scfstat.l \                   stmouse.r$(ODIR)/stclock: stclock.r  chd $(RDIR); l68 $(LFLAGS) -n=stclock -o=$@ $(SYSLIB) \      stclock.rstclock.r: stclock.a  r68 stclock.a -a=HYPERCACHE=0 -a=ROM=0 -o=$(RDIR)/$@ $(ODIR)/stclock.rom: stclock_rom.r  chd $(RDIR); l68 $(LFLAGS) -n=stclock -o=$@ $(SYSLIB) \      stclock_rom.rstclock_rom.r: stclock.a  r68 stclock.a -a=HYPERCACHE=0 -a=ROM=1 -o=$(RDIR)/$@ $(ODIR)/stclock.hc: stclock_hc.r  chd $(RDIR); l68 $(LFLAGS) -n=stclock -o=$@ $(SYSLIB) \      stclock_hc.rstclock_hc.r: stclock.a  r68 stclock.a -a=HYPERCACHE=1 -a=ROM=0 -o=$(RDIR)/$@ $(ODIR)/stclock.rom.hc: stclock_rom_hc.r  chd $(RDIR); l68 $(LFLAGS) -n=stclock -o=$@ $(SYSLIB) \      stclock_rom_hc.rstclock_rom_hc.r: stclock.a  r68 stclock.a -a=HYPERCACHE=1 -a=ROM=1 -o=$(RDIR)/$@ $(ODIR)/stinit.d0: stinit_d0.r  chd $(RDIR); l68 $(LFLAGS) -n=init -o=$@ $(SYSLIB) \                   stinit_d0.r$(ODIR)/stinit.h0: stinit_h0.r  chd $(RDIR); l68 $(LFLAGS) -n=init -o=$@ $(SYSLIB) \                   stinit_h0.r$(ODIR)/stsysgo: stsysgo.r  chd $(RDIR); l68 $(LFLAGS) -n=sysgo -o=$@ $(SYSLIB) \                   stsysgo.r$(ODIR)/stsysts: stsysts.r  chd $(RDIR); l68 $(LFLAGS) -n=sysgo -o=$@ $(SYSLIB) \                   stsysts.r