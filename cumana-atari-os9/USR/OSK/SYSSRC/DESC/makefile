## makefile for Cumana OS-9/68000 V2.3 device descriptors## Created: 03/19/90#RC     = r68#ODIR   = ../CMDS/BOOTOBJSRDIR   = RELSRFLAGS = -qSYSLIB = -l=/h0/LIB/sys.lall.date: floppy harddisk ramdisk console otherio vtd  @echo "ALL DONE"floppy: $(ODIR)/d0.def.0k     $(ODIR)/d0.3ms.0k     $(ODIR)/d0.5ms.0k     $(ODIR)/d0.6ms.0k         \        $(ODIR)/dd.d0.def.0k  $(ODIR)/dd.d0.3ms.0k  $(ODIR)/dd.d0.5ms.0k  $(ODIR)/dd.d0.6ms.0k      \        $(ODIR)/d0.def.32k    $(ODIR)/d0.3ms.32k    $(ODIR)/d0.5ms.32k    $(ODIR)/d0.6ms.32k        \        $(ODIR)/dd.d0.def.32k $(ODIR)/dd.d0.3ms.32k $(ODIR)/dd.d0.5ms.32k $(ODIR)/dd.d0.6ms.32k     \        $(ODIR)/d0.def.64k    $(ODIR)/d0.3ms.64k    $(ODIR)/d0.5ms.64k    $(ODIR)/d0.6ms.64k        \        $(ODIR)/dd.d0.def.64k $(ODIR)/dd.d0.3ms.64k $(ODIR)/dd.d0.5ms.64k $(ODIR)/dd.d0.6ms.64k     \        $(ODIR)/d1.def        $(ODIR)/d1.3ms        $(ODIR)/d1.5ms        $(ODIR)/d1.6ms            \        $(ODIR)/dd.d1.def     $(ODIR)/dd.d1.3ms     $(ODIR)/dd.d1.5ms     $(ODIR)/dd.d1.6ms         \                                                                                                    \        $(ODIR)/d0s.def.0k     $(ODIR)/d0s.3ms.0k     $(ODIR)/d0s.5ms.0k     $(ODIR)/d0s.6ms.0k     \        $(ODIR)/dd.d0s.def.0k  $(ODIR)/dd.d0s.3ms.0k  $(ODIR)/dd.d0s.5ms.0k  $(ODIR)/dd.d0s.6ms.0k  \        $(ODIR)/d0s.def.32k    $(ODIR)/d0s.3ms.32k    $(ODIR)/d0s.5ms.32k    $(ODIR)/d0s.6ms.32k    \        $(ODIR)/dd.d0s.def.32k $(ODIR)/dd.d0s.3ms.32k $(ODIR)/dd.d0s.5ms.32k $(ODIR)/dd.d0s.6ms.32k \        $(ODIR)/d0s.def.64k    $(ODIR)/d0s.3ms.64k    $(ODIR)/d0s.5ms.64k    $(ODIR)/d0s.6ms.64k    \        $(ODIR)/dd.d0s.def.64k $(ODIR)/dd.d0s.3ms.64k $(ODIR)/dd.d0s.5ms.64k $(ODIR)/dd.d0s.6ms.64k \        $(ODIR)/d1s.def        $(ODIR)/d1s.3ms        $(ODIR)/d1s.5ms        $(ODIR)/d1s.6ms        \        $(ODIR)/dd.d1s.def     $(ODIR)/dd.d1s.3ms     $(ODIR)/dd.d1s.5ms     $(ODIR)/dd.d1s.6ms     \                                                                                                    \        $(ODIR)/d0u.def.0k     $(ODIR)/d0u.3ms.0k     $(ODIR)/d0u.5ms.0k     $(ODIR)/d0u.6ms.0k     \        $(ODIR)/dd.d0u.def.0k  $(ODIR)/dd.d0u.3ms.0k  $(ODIR)/dd.d0u.5ms.0k  $(ODIR)/dd.d0u.6ms.0k  \        $(ODIR)/d0u.def.32k    $(ODIR)/d0u.3ms.32k    $(ODIR)/d0u.5ms.32k    $(ODIR)/d0u.6ms.32k    \        $(ODIR)/dd.d0u.def.32k $(ODIR)/dd.d0u.3ms.32k $(ODIR)/dd.d0u.5ms.32k $(ODIR)/dd.d0u.6ms.32k \        $(ODIR)/d0u.def.64k    $(ODIR)/d0u.3ms.64k    $(ODIR)/d0u.5ms.64k    $(ODIR)/d0u.6ms.64k    \        $(ODIR)/dd.d0u.def.64k $(ODIR)/dd.d0u.3ms.64k $(ODIR)/dd.d0u.5ms.64k $(ODIR)/dd.d0u.6ms.64k \        $(ODIR)/d1u.def        $(ODIR)/d1u.3ms        $(ODIR)/d1u.5ms        $(ODIR)/d1u.6ms        \        $(ODIR)/dd.d1u.def     $(ODIR)/dd.d1u.3ms     $(ODIR)/dd.d1u.5ms     $(ODIR)/dd.d1u.6ms     \                                                                                                    \        $(ODIR)/tos0.def.0k  @echo "FLOPPY DONE"harddisk: $(ODIR)/h0     $(ODIR)/h1     $(ODIR)/h2     $(ODIR)/h3     \          $(ODIR)/h4     $(ODIR)/h5     $(ODIR)/h6     $(ODIR)/h7     \          $(ODIR)/h8     $(ODIR)/h9     $(ODIR)/h10    $(ODIR)/h11    \          $(ODIR)/h12    $(ODIR)/h13    $(ODIR)/h14    $(ODIR)/h15    \          $(ODIR)/dd.h0  $(ODIR)/dd.h1  $(ODIR)/dd.h2  $(ODIR)/dd.h3  \          $(ODIR)/dd.h4  $(ODIR)/dd.h5  $(ODIR)/dd.h6  $(ODIR)/dd.h7  \          $(ODIR)/dd.h8  $(ODIR)/dd.h9  $(ODIR)/dd.h10 $(ODIR)/dd.h11 \          $(ODIR)/dd.h12 $(ODIR)/dd.h13 $(ODIR)/dd.h14 $(ODIR)/dd.h15  @echo "HARDDISK DONE"ramdisk: $(ODIR)/r0_32k     $(ODIR)/r0_64k      $(ODIR)/r0_128k     $(ODIR)/r0_256k     \         $(ODIR)/r0_384k    $(ODIR)/r0_512k     $(ODIR)/r0_1024k    $(ODIR)/r0_2048k    \         $(ODIR)/dd.r0_32k  $(ODIR)/dd.r0_64k   $(ODIR)/dd.r0_128k  $(ODIR)/dd.r0_256k  \         $(ODIR)/dd.r0_384k $(ODIR)/dd.r0_512k  $(ODIR)/dd.r0_1024k $(ODIR)/dd.r0_2048k  @echo "RAMDISK DONE"console: $(ODIR)/term.english      $(ODIR)/term.german      \         $(ODIR)/term.m110.english $(ODIR)/term.m110.german \         $(ODIR)/term.194.english  $(ODIR)/term.194.german  \         $(ODIR)/mouse  @echo "CONSOLE DONE"otherio: $(ODIR)/t1 $(ODIR)/t2 $(ODIR)/t3 $(ODIR)/t4 $(ODIR)/t5 $(ODIR)/t6 \         $(ODIR)/p  $(ODIR)/p.noshare $(ODIR)/iec  @echo "OTHERIO DONE"vtd: $(ODIR)/vterm0.german $(ODIR)/vterm0.english \     $(ODIR)/vterm1 $(ODIR)/vterm2 $(ODIR)/vterm3 $(ODIR)/vterm4 \     $(ODIR)/vterm5 $(ODIR)/vterm6 $(ODIR)/vterm7  @echo "VTD DONE"## Definitions for steprates#STEPDEF = 0STEP3MS = 1STEP5MS = 2STEP6MS = 3## floppy disk drives (d0)#$(ODIR)/d0.def.0k: d0_def_0k.r  chd $(RDIR); l68 -n=d0 -o=$@ $(SYSLIB) d0_def_0k.r$(ODIR)/dd.d0.def.0k: d0_def_0k.r  chd $(RDIR); l68 -n=dd -o=$@ $(SYSLIB) d0_def_0k.rd0_def_0k.r: d0.a  $(RC) $(RFLAGS) -o=$(RDIR)/$@ d0.a \        -A=CACHESIZE=0 -ASTEPRATE=$(STEPDEF)$(ODIR)/d0.3ms.0k: d0_3ms_0k.r  chd $(RDIR); l68 -n=d0 -o=$@ $(SYSLIB) d0_3ms_0k.r$(ODIR)/dd.d0.3ms.0k: d0_3ms_0k.r  chd $(RDIR); l68 -n=dd -o=$@ $(SYSLIB) d0_3ms_0k.rd0_3ms_0k.r: d0.a  $(RC) $(RFLAGS) -o=$(RDIR)/$@ d0.a \        -A=CACHESIZE=0 -ASTEPRATE=$(STEP3MS)$(ODIR)/d0.5ms.0k: d0_5ms_0k.r  chd $(RDIR); l68 -n=d0 -o=$@ $(SYSLIB) d0_5ms_0k.r$(ODIR)/dd.d0.5ms.0k: d0_5ms_0k.r  chd $(RDIR); l68 -n=dd -o=$@ $(SYSLIB) d0_5ms_0k.rd0_5ms_0k.r: d0.a  $(RC) $(RFLAGS) -o=$(RDIR)/$@ d0.a \        -A=CACHESIZE=0 -ASTEPRATE=$(STEP5MS)$(ODIR)/d0.6ms.0k: d0_6ms_0k.r  chd $(RDIR); l68 -n=d0 -o=$@ $(SYSLIB) d0_6ms_0k.r$(ODIR)/dd.d0.6ms.0k: d0_6ms_0k.r  chd $(RDIR); l68 -n=dd -o=$@ $(SYSLIB) d0_6ms_0k.rd0_6ms_0k.r: d0.a  $(RC) $(RFLAGS) -o=$(RDIR)/$@ d0.a \        -A=CACHESIZE=0 -ASTEPRATE=$(STEP6MS)$(ODIR)/d0.def.32k: d0_def_32k.r  chd $(RDIR); l68 -n=d0 -o=$@ $(SYSLIB) d0_def_32k.r$(ODIR)/dd.d0.def.32k: d0_def_32k.r  chd $(RDIR); l68 -n=dd -o=$@ $(SYSLIB) d0_def_32k.rd0_def_32k.r: d0.a  $(RC) $(RFLAGS) -o=$(RDIR)/$@ d0.a \        -A=CACHESIZE=1 -ASTEPRATE=$(STEPDEF)$(ODIR)/d0.3ms.32k: d0_3ms_32k.r  chd $(RDIR); l68 -n=d0 -o=$@ $(SYSLIB) d0_3ms_32k.r$(ODIR)/dd.d0.3ms.32k: d0_3ms_32k.r  chd $(RDIR); l68 -n=dd -o=$@ $(SYSLIB) d0_3ms_32k.rd0_3ms_32k.r: d0.a  $(RC) $(RFLAGS) -o=$(RDIR)/$@ d0.a \        -A=CACHESIZE=1 -ASTEPRATE=$(STEP3MS)$(ODIR)/d0.5ms.32k: d0_5ms_32k.r  chd $(RDIR); l68 -n=d0 -o=$@ $(SYSLIB) d0_5ms_32k.r$(ODIR)/dd.d0.5ms.32k: d0_5ms_32k.r  chd $(RDIR); l68 -n=dd -o=$@ $(SYSLIB) d0_5ms_32k.rd0_5ms_32k.r: d0.a  $(RC) $(RFLAGS) -o=$(RDIR)/$@ d0.a \        -A=CACHESIZE=1 -ASTEPRATE=$(STEP5MS)$(ODIR)/d0.6ms.32k: d0_6ms_32k.r  chd $(RDIR); l68 -n=d0 -o=$@ $(SYSLIB) d0_6ms_32k.r$(ODIR)/dd.d0.6ms.32k: d0_6ms_32k.r  chd $(RDIR); l68 -n=dd -o=$@ $(SYSLIB) d0_6ms_32k.rd0_6ms_32k.r: d0.a  $(RC) $(RFLAGS) -o=$(RDIR)/$@ d0.a \        -A=CACHESIZE=1 -ASTEPRATE=$(STEP6MS)$(ODIR)/d0.def.64k: d0_def_64k.r  chd $(RDIR); l68 -n=d0 -o=$@ $(SYSLIB) d0_def_64k.r$(ODIR)/dd.d0.def.64k: d0_def_64k.r  chd $(RDIR); l68 -n=dd -o=$@ $(SYSLIB) d0_def_64k.rd0_def_64k.r: d0.a  $(RC) $(RFLAGS) -o=$(RDIR)/$@ d0.a \        -A=CACHESIZE=2 -ASTEPRATE=$(STEPDEF)$(ODIR)/d0.3ms.64k: d0_3ms_64k.r  chd $(RDIR); l68 -n=d0 -o=$@ $(SYSLIB) d0_3ms_64k.r$(ODIR)/dd.d0.3ms.64k: d0_3ms_64k.r  chd $(RDIR); l68 -n=dd -o=$@ $(SYSLIB) d0_3ms_64k.rd0_3ms_64k.r: d0.a  $(RC) $(RFLAGS) -o=$(RDIR)/$@ d0.a \        -A=CACHESIZE=2 -ASTEPRATE=$(STEP3MS)$(ODIR)/d0.5ms.64k: d0_5ms_64k.r  chd $(RDIR); l68 -n=d0 -o=$@ $(SYSLIB) d0_5ms_64k.r$(ODIR)/dd.d0.5ms.64k: d0_5ms_64k.r  chd $(RDIR); l68 -n=dd -o=$@ $(SYSLIB) d0_5ms_64k.rd0_5ms_64k.r: d0.a  $(RC) $(RFLAGS) -o=$(RDIR)/$@ d0.a \        -A=CACHESIZE=2 -ASTEPRATE=$(STEP5MS)$(ODIR)/d0.6ms.64k: d0_6ms_64k.r  chd $(RDIR); l68 -n=d0 -o=$@ $(SYSLIB) d0_6ms_64k.r$(ODIR)/dd.d0.6ms.64k: d0_6ms_64k.r  chd $(RDIR); l68 -n=dd -o=$@ $(SYSLIB) d0_6ms_64k.rd0_6ms_64k.r: d0.a  $(RC) $(RFLAGS) -o=$(RDIR)/$@ d0.a \        -A=CACHESIZE=2 -ASTEPRATE=$(STEP6MS)## floppy disk drives (d1)#$(ODIR)/d1.def: d1_def.r  chd $(RDIR); l68 -n=d1 -o=$@ $(SYSLIB) d1_def.r$(ODIR)/dd.d1.def: d1_def.r  chd $(RDIR); l68 -n=dd -o=$@ $(SYSLIB) d1_def.rd1_def.r: d1.a  $(RC) $(RFLAGS) -o=$(RDIR)/$@ d1.a \        -ASTEPRATE=$(STEPDEF)$(ODIR)/d1.3ms: d1_3ms.r  chd $(RDIR); l68 -n=d1 -o=$@ $(SYSLIB) d1_3ms.r$(ODIR)/dd.d1.3ms: d1_3ms.r  chd $(RDIR); l68 -n=dd -o=$@ $(SYSLIB) d1_3ms.rd1_3ms.r: d1.a  $(RC) $(RFLAGS) -o=$(RDIR)/$@ d1.a \        -ASTEPRATE=$(STEP3MS)$(ODIR)/d1.5ms: d1_5ms.r  chd $(RDIR); l68 -n=d1 -o=$@ $(SYSLIB) d1_5ms.r$(ODIR)/dd.d1.5ms: d1_5ms.r  chd $(RDIR); l68 -n=dd -o=$@ $(SYSLIB) d1_5ms.rd1_5ms.r: d1.a  $(RC) $(RFLAGS) -o=$(RDIR)/$@ d1.a \        -ASTEPRATE=$(STEP5MS)$(ODIR)/d1.6ms: d1_6ms.r  chd $(RDIR); l68 -n=d1 -o=$@ $(SYSLIB) d1_6ms.r$(ODIR)/dd.d1.6ms: d1_6ms.r  chd $(RDIR); l68 -n=dd -o=$@ $(SYSLIB) d1_6ms.rd1_6ms.r: d1.a  $(RC) $(RFLAGS) -o=$(RDIR)/$@ d1.a \        -ASTEPRATE=$(STEP6MS)## floppy disk drives (d0s)#$(ODIR)/d0s.def.0k: d0s_def_0k.r  chd $(RDIR); l68 -n=d0s -o=$@ $(SYSLIB) d0s_def_0k.r$(ODIR)/dd.d0s.def.0k: d0s_def_0k.r  chd $(RDIR); l68 -n=dd -o=$@ $(SYSLIB) d0s_def_0k.rd0s_def_0k.r: d0s.a  $(RC) $(RFLAGS) -o=$(RDIR)/$@ d0s.a \        -A=CACHESIZE=0 -ASTEPRATE=$(STEPDEF)$(ODIR)/d0s.3ms.0k: d0s_3ms_0k.r  chd $(RDIR); l68 -n=d0s -o=$@ $(SYSLIB) d0s_3ms_0k.r$(ODIR)/dd.d0s.3ms.0k: d0s_3ms_0k.r  chd $(RDIR); l68 -n=dd -o=$@ $(SYSLIB) d0s_3ms_0k.rd0s_3ms_0k.r: d0s.a  $(RC) $(RFLAGS) -o=$(RDIR)/$@ d0s.a \        -A=CACHESIZE=0 -ASTEPRATE=$(STEP3MS)$(ODIR)/d0s.5ms.0k: d0s_5ms_0k.r  chd $(RDIR); l68 -n=d0s -o=$@ $(SYSLIB) d0s_5ms_0k.r$(ODIR)/dd.d0s.5ms.0k: d0s_5ms_0k.r  chd $(RDIR); l68 -n=dd -o=$@ $(SYSLIB) d0s_5ms_0k.rd0s_5ms_0k.r: d0s.a  $(RC) $(RFLAGS) -o=$(RDIR)/$@ d0s.a \        -A=CACHESIZE=0 -ASTEPRATE=$(STEP5MS)$(ODIR)/d0s.6ms.0k: d