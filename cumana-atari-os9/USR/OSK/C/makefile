##	Makefile for *example* user trap handler##mytrap: Rels/trapmain.r Rels/mytrap.l Rels/trapfuncs.l	l68 Rels/trapmain.r Rels/trapfuncs.l -l=/dd/lib/sys.l -o=mytrap -n=mytrap -M=2k -g	attr -pee -x mytrapRels/mytrap.l: Rels/mytraps.r Rels/trapdefs.r Rels/inittrap.r	merge >-Rels/mytrap.l Rels/mytraps.r Rels/trapdefs.r Rels/inittrap.rRels/trapmain.r: trapmain.a	r68 trapmain.a -o=Rels/trapmain.r -qRels/mytraps.r: mytraps.a	r68 -qo=Rels/mytraps.r mytraps.aRels/trapdefs.r: trapdefs.a	r68 -qo=Rels/trapdefs.r trapdefs.aRels/inittrap.r: inittrap.a	r68 -qo=Rels/inittrap.r inittrap.a## likely something more substantial here...#Rels/trapfuncs.l: Rels/funcs.r	merge >-Rels/trapfuncs.l Rels/funcs.rRels/funcs.r: funcs.c	cc -sqgr=Rels funcs.c## example program accessing mytrap handler and library#Rels/example.r: example.c	cc -qgt=/dd example.c -r=Relsexample_trap: Rels/example.r	l68 /dd/lib/cstart.r Rels/example.r -l=Rels/mytrap.l -l=/dd/lib/clib.l\	-l=/dd/lib/math.l -l=/dd/lib/sys.l -go=example_trapexample_notrap: Rels/example.r	l68 /dd/lib/cstart.r Rels/example.r -l=Rels/trapfuncs.l -l=/dd/lib/clibn.l\	-l=/dd/lib/math.l -l=/dd/lib/sys.l -go=example_notrap