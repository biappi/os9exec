 nam scr ttl stcon/stvir getstat/setstat bindings********************************************************************                                                                 **       Dieses Programm ist urheberrechtlich geschuetzt!!!        **                                                                 **  Copyrights (c) 1988 by recc-O-ware, Wolfgang Ocker, Puchheim   **                                                                 **************************************************************************************************************************************** Editon History  scr.a**  #    Date      Comments                                      By* -- --------- ------------------------------------------------ ---* 00  11/03/88 Fixed                                            weoEdition equ 0 psectSS_Screen equ 200 ; Screen getstat/setstat function code**************************** _ss_screen(fd, fc, count)*   int fd, fc, count;*_ss_screen:  movem.l a0/d2/d3,-(a7) ; save registers move.w d1,d2 ; screen function code move.w #SS_Screen,d1 move.l 16(a7),d3 ; count move.l d3,a0 OS9 I$SetSttscr05 bcc.s scr10 move.l d1,errno(a6) ; save error number moveq.l #-1,d0 bra.s scr20scr10 moveq.l #0,d0 ; no errorscr20 movem.l (a7)+,a0/d2/d3 ; restore registers rts********************************** _ss_screenx(fd, fc, buf, count)*   int  fd, fc;*   char *buf;*   int  count;*_ss_screenx:  movem.l a0/d2/d3,-(a7) ; save registers move.w d1,d2 ; screen function code move.w #SS_Screen,d1 move.l 16(a7),a0 ; buffer move.l 20(a7),d3 ; count OS9 I$SetSttscrx05 bcc.s scrx10 move.l d1,errno(a6) ; save error number moveq.l #-1,d0 bra.s scrx20scrx10 moveq.l #0,d0 ; no errorscrx20 movem.l (a7)+,a0/d2/d3 rts************************* _gs_screen(fd, screen)*   int    fd;*   SCREEN *screen;*_gs_screen: movem.l a0/d2/d3,-(a7) ; save registers move.l d1,a0 ; screen info buffer move.w #SS_Screen,d1 moveq.l #0,d2 OS9 I$GetStt bra.s scr05*********************** _gs_palette(fd, buf)*   int    fd;*   SCREEN *screen;*_gs_palette: movem.l a0/d2/d3,-(a7) ; save registers move.l d1,a0 ; palette info buffer move.w #SS_Screen,d1 move.w #22,d2 ; ScrPalette OS9 I$GetStt bra.s scr05*********************** _ss_palette(fd, buf)*   int    fd;*   SCREEN *screen;*_ss_palette: movem.l a0/d2/d3,-(a7) ; save registers move.l d1,a0 ; palette info buffer move.w #SS_Screen,d1 move.w #22,d2 ; ScrPalette OS9 I$SetStt bra.s scr05 ends