## makefile for utlities#CFLAGS = -qt=/dd -xiRDIR   = RELSODIR   = ../CMDSLIB    = /h0/LIBall: $(LIB)/scr.r $(LIB)/dsk.r $(ODIR)/setscreen $(ODIR)/mount $(ODIR)/hdpark  @touch all.date  @echo "ALL DONE"$(LIB)/scr.r: scr.a$(LIB)/dsk.r: dsk.a$(ODIR)/setscreen: setscreen.r $(LIB)/scr.r  chd $(RDIR); cc $(CFLAGS) setscreen.r $(LIB)/scr.r -xiq -n=setscreen \                  -fd=../$@  attr $@ -epe$(ODIR)/mount: mount.r $(LIB)/dsk.r  chd $(RDIR); cc $(CFLAGS) mount.r $(LIB)/dsk.r -n=mount \                  -fd=../$@  attr $@ -epe$(ODIR)/hdpark: hdpark.r $(LIB)/dsk.r  chd $(RDIR); cc $(CFLAGS) hdpark.r $(LIB)/dsk.r -n=hdpark \                  -fd=../$@  attr $@ -epe