##	Makefile for cache##ODIR = .../CMDSRDIR = RELSall: cttrap cache  merge -x cttrap >+$(ODIR)/cache  touch all.datecttrap: $(RDIR)/trapmain.r $(RDIR)/cttrap.l $(RDIR)/trapfuncs.l	l68 $(RDIR)/trapmain.r $(RDIR)/trapfuncs.l -l=/h0/LIB/sys.l -o=cttrap \            -n=cttrap -M=2k -g	attr -pee -x $@$(RDIR)/cttrap.l: $(RDIR)/cttraps.r $(RDIR)/trapdefs.r $(RDIR)/inittrap.r 	merge >-$(RDIR)/cttrap.l $(RDIR)/cttraps.r $(RDIR)/trapdefs.r $(RDIR)/inittrap.r$(RDIR)/trapmain.r: trapmain.a	r68 trapmain.a -o=$(RDIR)/trapmain.r -q$(RDIR)/cttraps.r: cttraps.a	r68 -qo=$(RDIR)/cttraps.r cttraps.a$(RDIR)/trapdefs.r: trapdefs.a	r68 -qo=$(RDIR)/trapdefs.r trapdefs.a$(RDIR)/inittrap.r: inittrap.a	r68 -qo=$(RDIR)/inittrap.r inittrap.a## likely something more substantial here...#$(RDIR)/trapfuncs.l: $(RDIR)/cachefuncs.r	merge >-$(RDIR)/trapfuncs.l $(RDIR)/cachefuncs.r$(RDIR)/cachefuncs.r: cachefuncs.a	r68 -qo=$(RDIR)/cachefuncs.r cachefuncs.a###$(RDIR)/cache.r: cache.c	cc -xit=/dd cache.c -r=$(RDIR)cache: $(RDIR)/cache.r	cc -xit=/dd $(RDIR)/cache.r -l=$(RDIR)/cttrap.l -f=$@