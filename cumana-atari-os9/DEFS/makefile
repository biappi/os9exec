# make file to update system librariesLDIR  = ../LIBRDIR  = RELSSYSL  = sysio.r sysglob.rUSRL  = funcs.r io.r module.r process.rRFLAGS= -q$(LDIR)/sys.l: $(USRL) $(SYSL) $(LDIR)/scfstat.l $(LDIR)/drvs1.l	chd $(RDIR); merge $(USRL) >-.$(LDIR)/usr.l	chd $(RDIR); merge $(SYSL)   .$(LDIR)/usr.l >-.$(LDIR)/sys.l	touch $(LDIR)/sys.lsysio.r:   sysio.a iodev.a rbfdev.a scfdev.a cdfmdev.a ucmdev.asysglob.r: sysglob.a ioglob.ascfstat.r: scfstat.a iodev.a scfdev.a$(LDIR)/scfstat.l: scfstat.r  merge $(RDIR)/scfstat.r >-$(LDIR)/scfstat.l$(LDIR)/drvs1.l: rbfstat.r drvstat.r  merge $(RDIR)/rbfstat.r $(RDIR)/drvstat.r >-$(LDIR)/drvs1.l  merge $(LDIR)/drvs1.l $(RDIR)/drvstat.r >-$(LDIR)/drvs2.l  merge $(LDIR)/drvs2.l $(RDIR)/drvstat.r >-$(LDIR)/drvs3.l  merge $(LDIR)/drvs3.l $(RDIR)/drvstat.r >-$(LDIR)/drvs4.l  merge $(LDIR)/drvs4.l $(RDIR)/drvstat.r $(RDIR)/drvstat.r $(RDIR)/drvstat.r \		$(RDIR)/drvstat.r >-$(LDIR)/drvs8.l  merge $(LDIR)/drvs8.l $(RDIR)/drvstat.r $(RDIR)/drvstat.r $(RDIR)/drvstat.r \		$(RDIR)/drvstat.r >-$(LDIR)/drvs12.l  merge $(LDIR)/drvs12.l $(RDIR)/drvstat.r $(RDIR)/drvstat.r $(RDIR)/drvstat.r \		$(RDIR)/drvstat.r $(RDIR)/drvstat.r $(RDIR)/drvstat.r \		$(RDIR)/drvstat.r $(RDIR)/drvstat.r >-$(LDIR)/drvs20.lrbfstat.r: rbfstat.a iodev.a rbfdev.adrvstat.r: drvstat.a iodev.a rbfdev.a