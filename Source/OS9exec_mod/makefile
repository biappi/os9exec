# Makefile for os9exec_mod# ------------------------MWOS    = .DEFSDIR = DEFSSYSRELS = LIBODIR    = .RDIR    = .ULIB    = $(SYSRELS)/usr.lSYSDEFS = $(DEFSDIR)/oskdefs.dRFLAGS  = -q -u=$(DEFSDIR)LFLAGS  = -f=oepeprRC      = r68# link rulesos9exec : os9exec_mod.r l68 $(LFLAGS) -l=$(ULIB) -O=$(ODIR)/os9exec $(RDIR)/os9exec_mod.r -n=OS9exec_ newname OS9exec -o=0 # assembly rulesos9exec_mod.r : os9exec_mod.a r68 -o=$(RDIR)/os9exec_mod.r $(RFLAGS) os9exec_mod.a